<html>
<head>
    <meta charset="utf-8" />
    <title>Fast Rabbit</title>
    <meta name="description" content="Amolexis">
    <meta name="author" content="">
    <meta name="keywords" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.0/css/bootstrap.min.css" integrity="sha384-9gVQ4dYFwwWSjIDZnLEWnxCjeSWFphJiwGPXr1jddIhOegiu1FwO5qRGvFXOdJZ4" crossorigin="anonymous">
    <style>
        @font-face {
            font-family: 'CircularStd-Book';
            src: url('fonts/CircularStd-Book/CircularStd-Book.eot');
            src: url('fonts/CircularStd-Book/CircularStd-Book.eot?#iefix') format('embedded-opentype'), url('fonts/CircularStd-Book/CircularStd-Book.woff2') format('woff2'), url('fonts/CircularStd-Book/CircularStd-Book.woff') format('woff'), url('fonts/CircularStd-Book/CircularStd-Book.ttf') format('truetype'), url('fonts/CircularStd-Book/CircularStd-Book.svg#svgFontName') format('svg');
            font-weight: normal!important;
        }
        body{
            
        }
        .box{
            max-width: 320px;
            margin: 80px auto;
        }
        .box .row{
            margin-bottom: 15px;
        }
        .box-head{
            margin-bottom: 50px;
        }
        .box-head .title{
            font-size: 25px;
            color: #504E50;
        }
        .box-head .des{
            color: #6B686B;
            font-size: 12px;
        }
        .box-list .name{
            color: #797474;
            font-size: 14px;
            margin-bottom: 4px;
        }
        .box-list .user{
            color: #4D4A4C;
            font-size: 22px;
        }
        .box-list .time{
            color: #5D9D3B;
            font-size: 10px;
            margin-top: 5px;
            text-align: right;
        }
        @media screen and (max-width:767px) {
            
        }
    </style>
</head>
<body>
    <div class="container">
        <div class="box">
            <div class="box-head">
                <h1 class="title">Live Access to Server</h1>
                <h5 class="des">Fast Rabbit Big Box</h5>
            </div>
            <div class="box-list">
                <% (data || []).forEach(item => { %>
                <div class="row">
                    <div class="col-md-8 col-sm-8 col-xs-8">
                        <p class="name"><%= item.productName %></p>
                        <p class="user"><%= item.count.toLocaleString() %> users</p>
                    </div>
                    <!-- <div class="col-md-4 col-sm-4 col-xs-4">
                        <p class="time">5 mins ago</p>
                    </div> -->
                </div>
                <% }) %>
            </div>
        </div>
    </div>
</body>
</html>